{
  "name": "vue3-element-admin-ts",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run serve \" \"npm run mock \" ",
    "serve": "vue-cli-service serve",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "build:preview": "vue-cli-service build --report --mode preview ",
    "build:prod": "vue-cli-service build --report",
    "svgo": "svgo -f src/icons/svg --config=src/icons/svgo.js",
    "mock": "json-server --watch ./mock/db.json",
    "view": "node script/view.js",
    "dsl": "node script/dsl.js"
  },
  "dependencies": {
    "@element-plus/icons": "0.0.11",
    "axios": "^0.24.0",
    "core-js": "^3.6.5",
    "echarts": "^5.2.2",
    "element-plus": "^1.2.0-beta.6",
    "js-cookie": "^3.0.1",
    "md5": "^2.3.0",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "path-to-regexp": "^6.2.0",
    "vue": "^3.2.16",
    "vue-router": "^4.0.11",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@types/jest": "^24.0.19",
    "@types/js-cookie": "^3.0.1",
    "@types/md5": "^2.3.1",
    "@types/node": "^16.11.11",
    "@types/nprogress": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-standard": "^5.1.2",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vue/test-utils": "^2.0.0-0",
    "concurrently": "^6.4.0",
    "ejs": "^3.1.6",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^7.0.0",
    "fs-extra": "^10.0.0",
    "json-server": "^0.17.0",
    "lint-staged": "^9.5.0",
    "prettier": "^2.5.0",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "script-ext-html-webpack-plugin": "^2.1.5",
    "svg-sprite-loader": "^6.0.11",
    "svgo": "^2.8.0",
    "typescript": "^4.1.6",
    "unplugin-auto-import": "^0.5.5",
    "unplugin-vue-components": "^0.17.11",
    "vue-cli-plugin-element-plus": "0.0.13",
    "vue-jest": "^5.0.0-0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "vue-cli-service lint",
      "prettier --write",
      "git add"
    ]
